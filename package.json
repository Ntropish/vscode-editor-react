{
  "name": "chat-completion",
  "displayName": "chat-completion",
  "description": "Adds an editor for chat files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chat-completion.helloWorld",
        "title": "Hello World"
      }
    ],
    "customEditors": [
      {
        "viewType": "chatView",
        "displayName": "Chat Completion Editor",
        "selector": [
          {
            "filenamePattern": "*.chat"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "tsc:build": "tsc -p tsconfig.build.json ",
    "tsc:watch": "tsc -p tsconfig.build.json  --watch",
    "vite:build": "vite build",
    "vite:watch": "vite build --watch",
    "build": "npm run tsc:build && npm run vite:build",
    "watch": "concurrently \"npm run tsc:watch\" \"npm run vite:watch\"",
    "preview": "vite preview",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  }
}
